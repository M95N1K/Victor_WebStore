@model IEnumerable<Victor_WebStore.ViewModels.EmployeeViewModel>
@{
    ViewData["Title"] = "Все работники";
}
<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>
                @Html.ActionLink("Детали", "EmployeeDetails", new { id = item.Id })
            </td>
        </tr>
    }
</table>
