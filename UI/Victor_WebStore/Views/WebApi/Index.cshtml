@model IEnumerable<string>
@{
    ViewData["Title"] = "Тест WebApi";
}



<div class="container">
    <h2>@ViewData["Title"]</h2>
    <hr />
    
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @{ int i = 1;}
            @foreach (var value in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>value</td>
                </tr>
            }
        </tbody>
    </table>
</div>